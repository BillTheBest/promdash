<h2>Server '<%= @server.name %>'</h2>
<p>
  <strong>Name:</strong>
  <%= @server.name %>
</p>

<p>
  <strong>URL:</strong>
  <%= @server.url %>
</p>

<p>
  <strong>Server type:</strong>
  <%= @server.server_type.capitalize %>
</p>

<div class="actions">
  <%= link_to 'Edit', edit_server_path(@server), class: "btn btn-primary" %>
  <%= link_to 'Back', servers_path, class: "btn btn-default" %>
</div>
